# GitHub API Configuration
GITHUB_TOKEN=your_github_personal_access_token_here

# OpenAI API Configuration (if using OpenAI for AI analysis)
OPENAI_API_KEY=your_openai_api_key_here

# Other AI Service APIs (uncomment and configure as needed)
# ANTHROPIC_API_KEY=your_anthropic_api_key_here
# GOOGLE_AI_API_KEY=your_google_ai_api_key_here
# AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
# AZURE_OPENAI_ENDPOINT=your_azure_openai_endpoint_here

# Application Configuration
VITE_APP_NAME=AI Issues Finder
VITE_APP_VERSION=1.0.0

# Development Configuration
VITE_DEV_MODE=true
VITE_API_BASE_URL=http://localhost:3000

# Production Configuration (for deployment)
# VITE_API_BASE_URL=https://your-production-api.com
# VITE_DEV_MODE=false

# Analytics (optional)
# VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
# VITE_SENTRY_DSN=your_sentry_dsn_here

# Note: 
# 1. Copy this file to .env and fill in your actual API keys
# 2. Never commit .env files to version control
# 3. Keep your API keys secure and rotate them regularly
# 4. Use environment-specific files (.env.local, .env.production) as needed